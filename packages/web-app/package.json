{
  "name": "web-app",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:type": "tsc --noEmit",
    "test:unit": "echo 'no test found'",
    "test:component": "npm run storybook:build && npm run storybook:static-test",
    "test:component-dev": "npm run storybook:test-runner -- 'npm run storybook -- --ci --quiet' 'npm run storybook:wait-test'",
    "storybook": "storybook dev -p 6006",
    "storybook:test-runner": "npx concurrently -k -s first -n 'sb:app,sb:test' -c 'magenta,blue'",
    "storybook:test": "test-storybook --url http://localhost:6006",
    "storybook:wait": "npx wait-on tcp:6006",
    "storybook:wait-test": "npm run storybook:wait && npm run storybook:test",
    "storybook:build": "storybook build -- --loglevel=verbose",
    "storybook:static-serve": "npx http-server storybook-static --port 6006 --silent",
    "storybook:static-test": "npm run storybook:test-runner -- 'npm run storybook:static-serve' 'npm run storybook:wait-test'"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.7.0",
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "framer-motion": "^10.12.16",
    "i18next": "^22.4.15",
    "i18next-browser-languagedetector": "^7.0.1",
    "i18next-http-backend": "^2.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.2.2",
    "react-icons": "^4.9.0",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.11.2"
  },
  "devDependencies": {
    "@storybook/react-vite": "^7.0.18",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^4.0.0",
    "prop-types": "^15.8.1",
    "typescript": "^5.0.2",
    "vite": "^4.3.9"
  }
}
